<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bynn.marketll.module_home">

    <application
        android:name="com.bynn.lib_basic.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/home_app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/HomeAppTheme">
        <activity
            android:name=".activity.SpecialInfoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.AppModuleActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ProductIntroductionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.HomeActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>