<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bynn.marketll.module_mine">

    <application
        android:name="com.bynn.lib_basic.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/mine_app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MineAppTheme">
        <activity
            android:name=".activity.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.MineCouponActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SeeMoreActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".activity.MineActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.CustomerServiceActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>